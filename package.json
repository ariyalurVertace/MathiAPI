{
    "name": "api",
    "version": "0.1.0",
    "private": true,
    "bin": "./src/server.js",
    "type": "module",
    "scripts": {
        "start": "nodemon src/server.js --exec babel-node src/server.js",
        "pretty": "prettier --ignore-path .gitignore --ignore-path .eslintignore --write --tab-width 4 \"**/*.js\"",
        "lint": "./node_modules/.bin/eslint . --ignore-path .gitignore --ignore-path .eslintignore --fix",
        "dev": "nodemon ./src/server.js",
        "debug": "nodemon --inspect ./src/server.js",
        "generate-crud": "node ./generator/controller/generate_crud.mjs",
        "generate-ui": "node ./generator/controller/generate_UI.mjs",
        "delete-removed-branches": "git fetch -p && for branch in `git branch -vv | grep ': gone]' | awk '{print $1}'`; do git branch -D $branch; done"
    },
    "lint-staged": {
        "*.js": [
            "prettier --ignore-path .gitignore --write --tab-width 4",
            "eslint --ignore-path .gitignore --fix",
            "git add"
        ],
        "*.{json,md,scss,yaml,yml}": [
            "prettier --ignore-path .gitignore --write --tab-width 4",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@prisma/client": "^2.30.3",
        "aws-sdk": "^2.943.0",
        "bluebird": "3.7.0",
        "body-parser": "1.19.0",
        "chalk": "2.4.2",
        "cors": "2.8.5",
        "dotenv": "8.1.0",
        "express": "4.17.1",
        "express-mung": "0.5.1",
        "got": "^11.8.2",
        "greenlock": "2.8.7",
        "helmet": "4.1.1",
        "http": "0.0.0",
        "json-query": "^2.2.2",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.15",
        "moment": "2.24.0",
        "morgan": "1.9.1",
        "multer": "^1.4.2",
        "node-schedule": "^2.0.0",
        "prisma": "^2.30.3",
        "redis": "^3.1.2",
        "redis-lock": "^0.1.4",
        "request-ip": "2.1.3",
        "socket.io": "^4.1.2",
        "socket.io-redis": "^6.1.1",
        "spdy": "4.0.1",
        "tweetnacl": "1.0.3",
        "tweetnacl-util": "0.15.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/cli": "7.6.4",
        "@babel/core": "7.6.4",
        "@babel/node": "^7.14.7",
        "@babel/plugin-proposal-optional-chaining": "7.6.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "7.6.3",
        "babel-eslint": "10.0.3",
        "eslint": "6.5.1",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-config-prettier": "6.4.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-prettier": "3.1.1",
        "eslint-plugin-promise": "4.2.1",
        "husky": "3.0.9",
        "inquirer": "7.0.0",
        "lint-staged": "9.4.2",
        "nodemon": "^1.19.3",
        "prettier": "1.18.2"
    }
}
